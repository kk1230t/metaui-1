{
  "name": "metaui",
  "version": "1.0.0",
  "description": "metaui",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "config": {
    "customUrl": "http://pug-starter.codetap.academy",
    "githubUrl": "http://github.com/marianzburlea/pug-starter.git",
    "deployToGithubIo": true,
    "directory": {
      "development": "tmp",
      "production": "build",
      "ghpages": ".publish",
      "source": "src",
      "data": "_data",
      "component": "_component",
      "layout": "_layout",
      "asset": "_asset",
      "image": "image",
      "font": "font",
      "templateCollection": [
        "article",
        "product"
      ]
    },
    "render": {
      "sourceFileChange": false,
      "allPugFiles": false,
      "url": {
        "//": "this is recommended to avoid Linux/Windows capitalization issue",
        "htmlExtensionOn": false,
        "toLowercase": true
      }
    },
    "entry": {
      "cssExternal": "style**.{scss,sass}",
      "cssEmbed": "embed**.{scss,sass}",
      "cssModular": false,
      "css": {
        "embed": false,
        "external": true
      }
    },
    "autoLaunchBrowser": true
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "del": "^5.1.0",
    "gulp": "^4.0.2"
  },
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/preset-stage-1": "^7.8.3",
    "@babel/register": "^7.18.9",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/stream": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.0",
    "browser-sync": "^2.27.10",
    "bs-eslint-message": "^0.0.3",
    "bulma": "^0.9.4",
    "constantinople": "^4.0.1",
    "core-js": "^3.26.0",
    "eslint": "^8.27.0",
    "foldero": "^0.1.1",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^4.0.3",
    "gulp-changed-in-place": "^2.3.0",
    "gulp-debug": "^4.0.0",
    "gulp-ejs": "^5.1.0",
    "gulp-eslint": "^6.0.0",
    "gulp-express": "^0.3.5",
    "gulp-gh-pages": "^0.5.4",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^8.0.0",
    "gulp-inline-source": "^4.0.0",
    "gulp-less": "^5.0.0",
    "gulp-load-plugins": "^2.0.8",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rollup": "^2.17.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-variables": "^1.2.0",
    "gulp-sourcemaps": "^3.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "jstransformer-markdown-it": "^3.0.0",
